<!--                        connectionURL="jdbc:mysql://192.168.24.100:3306/message?useUnicode=true&amp;characterEncoding=UTF-8"-->
<!--                        userId="root"-->
<!--                        password="8812345!@#">-->
<!--        </jdbcConnection>-->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://47.98.62.0:3306/vi_cash_dog?useUnicode=true&amp;characterEncoding=UTF-8"
                        userId="root"
                        password="Dolphin0408+">
        </jdbcConnection>
        <!-- 类型转换 -->
        <javaTypeResolver >
            <!-- 是否使用bigDecimal， false可自动转化以下类型（Long, Integer, Short, etc.） -->
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>
        <!-- 生成实体类地址 -->
        <javaModelGenerator targetPackage="com.panshi.hujin2.message.dao.model.fcm" targetProject="src/main/java">
            <property name="enableSubPackages" value="true" /> <!-- 是否在当前路径下新加一层-->
            <property name="trimStrings" value="true" /> <!-- 是否针对string类型的字段在set的时候进行trim调用 -->
        </javaModelGenerator>
        <!-- 生成MAPXML文件 -->
        <sqlMapGenerator targetPackage="mapper"  targetProject="src/main/resources">
            <property name="enableSubPackages" value="true" /> <!-- 是否在当前路径下新加一层-->
        </sqlMapGenerator>
        <!-- 生成DAO -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.panshi.hujin2.message.dao.mapper.fcm" targetProject="src/main/java">
            <property name="enableSubPackages" value="true" /> <!-- 是否在当前路径下新加一层-->
        </javaClientGenerator>
        <!-- 配置表信息 -->
        <table schema="" tableName="user_one_signal_relation" domainObjectName="UserOneSignalRelation"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false" >

            <!--insert返回自增主键-->
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>
    </context>
</generatorConfiguration>
